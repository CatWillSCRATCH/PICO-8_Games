	if bs>19 then bq+=1 end
	if a!=0 then
		spr(128,851,120+min(bq/15,18))
	end
	elseif a==2 then
		print("dungeon!",50,21,5)
		print("dungeon!",49,20,7)
		print(" made by\ndeklaswas",47,104,3)
		print("press anything to start",20,64,6)
	elseif a==3 then
